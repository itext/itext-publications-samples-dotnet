    <ProjectReference Include="..\..\..\pdfocr\itext\itext.pdfocr.onnxtr\itext.pdfocr.onnxtr.csproj" Condition="Exists('..\..\..\pdfocr\itext\itext.pdfocr.onnxtr\itext.pdfocr.onnxtr.csproj')" />
